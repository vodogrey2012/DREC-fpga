#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2018ea0 .scope module, "tb" "tb" 2 3;
 .timescale -9 -11;
v0x20425e0_0 .var "clk", 0 0;
v0x20426d0_0 .net "dclk", 0 0, L_0x2042bb0; 1 drivers
v0x2042750_0 .var "en", 0 0;
v0x20427d0_0 .var "i", 2 0;
v0x2042880_0 .var/real "period", 0 0;
v0x2042900_0 .var "rstn", 0 0;
v0x2042a10_0 .var "sel", 1 0;
v0x2042ae0_0 .var "unused", 0 0;
S_0x2042090 .scope task, "clkcheck" "clkcheck" 2 43, 2 43, S_0x2018ea0;
 .timescale -9 -11;
v0x20421b0_0 .var "en_t", 0 0;
v0x2042270_0 .var/i "exp", 31 0;
v0x2042310_0 .var/i "factor", 31 0;
v0x20423b0_0 .var/real "period_t", 0 0;
v0x2042460_0 .var "sel_t", 1 0;
v0x2042500_0 .var/real "t", 0 0;
E_0x2042180 .event posedge, v0x2041d70_0;
TD_tb.clkcheck ;
    %load/v 8, v0x2042460_0, 2;
    %set/v v0x2042a10_0, 8, 2;
    %load/v 8, v0x20421b0_0, 1;
    %set/v v0x2042750_0, 8, 1;
    %set/v v0x2042900_0, 0, 1;
    %delay 500, 0;
    %set/v v0x2042900_0, 1, 1;
    %wait E_0x2042180;
    %vpi_func/r 2 62 "$realtime", 4;
    %set/wr v0x2042500_0, 4;
    %wait E_0x2042180;
    %vpi_func/r 2 64 "$realtime", 4;
    %load/wr 5, v0x2042500_0;
    %sub/wr 4, 5;
    %set/wr v0x2042500_0, 4;
    %movi 8, 2, 32;
    %movi 40, 8, 32;
    %load/v 72, v0x2042460_0, 2;
    %movi 74, 0, 30;
    %sub 40, 72, 32;
    %pow/s 8, 40, 32;
    %set/v v0x2042310_0, 8, 32;
    %load/wr 4, v0x20423b0_0;
    %load/v 8, v0x2042310_0, 32;
    %ix/get/s 5, 8, 32;
    %cvt/ri 5, 5;
    %mul/wr 4, 5;
    %cvt/vr 8, 4, 32;
    %set/v v0x2042270_0, 8, 32;
    %load/wr 4, v0x2042500_0;
    %load/v 9, v0x2042270_0, 32;
    %ix/get/s 5, 9, 32;
    %cvt/ri 5, 5;
    %cmp/wr 4, 5;
    %mov 8, 4, 1;
    %inv 8, 1;
    %jmp/0xz  T_0.0, 8;
    %vpi_call 2 70 "$display", "ERROR!";
    %delay 10000, 0;
    %vpi_call 2 72 "$finish";
T_0.0 ;
    %end;
S_0x1ff3690 .scope module, "top_inst" "top" 2 12, 3 1, S_0x2018ea0;
 .timescale -9 -11;
L_0x2042bb0 .functor AND 1, v0x2041a00_0, v0x2042750_0, C4<1>, C4<1>;
v0x2041bf0_0 .net "clk_i", 0 0, v0x20425e0_0; 1 drivers
v0x2041cc0_0 .net "dclk", 0 0, v0x2041a00_0; 1 drivers
v0x2041d70_0 .alias "dclk_o", 0 0, v0x20426d0_0;
v0x2041df0_0 .net "en_i", 0 0, v0x2042750_0; 1 drivers
v0x2041ea0_0 .net "rstn_i", 0 0, v0x2042900_0; 1 drivers
v0x2041f50_0 .net "sel_i", 1 0, v0x2042a10_0; 1 drivers
v0x2042010_0 .net "unused_o", 0 0, C4<z>; 0 drivers
S_0x1ff3780 .scope module, "clkdiv_inst" "clkdiv" 3 19, 4 3, S_0x1ff3690;
 .timescale -9 -11;
v0x1ff3870_0 .alias "clk_i", 0 0, v0x2041bf0_0;
v0x2041960_0 .var "cnt", 7 0;
v0x2041a00_0 .var "dclk_o", 0 0;
v0x2041aa0_0 .alias "rstn_i", 0 0, v0x2041ea0_0;
v0x2041b50_0 .alias "sel_i", 1 0, v0x2041f50_0;
E_0x2006c90 .event edge, v0x2041b50_0, v0x2041960_0;
E_0x1ff3600/0 .event negedge, v0x2041aa0_0;
E_0x1ff3600/1 .event posedge, v0x1ff3870_0;
E_0x1ff3600 .event/or E_0x1ff3600/0, E_0x1ff3600/1;
    .scope S_0x1ff3780;
T_1 ;
    %wait E_0x1ff3600;
    %load/v 8, v0x2041aa0_0, 1;
    %inv 8, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x2041960_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v0x2041960_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v0x2041960_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1ff3780;
T_2 ;
    %wait E_0x2006c90;
    %load/v 8, v0x2041b50_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_2.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_2.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_2.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_2.3, 6;
    %set/v v0x2041a00_0, 0, 1;
    %jmp T_2.5;
T_2.0 ;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.6, 4;
    %load/x1p 8, v0x2041960_0, 1;
    %jmp T_2.7;
T_2.6 ;
    %mov 8, 2, 1;
T_2.7 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x2041a00_0, 8, 1;
    %jmp T_2.5;
T_2.1 ;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.8, 4;
    %load/x1p 8, v0x2041960_0, 1;
    %jmp T_2.9;
T_2.8 ;
    %mov 8, 2, 1;
T_2.9 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x2041a00_0, 8, 1;
    %jmp T_2.5;
T_2.2 ;
    %ix/load 1, 5, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.10, 4;
    %load/x1p 8, v0x2041960_0, 1;
    %jmp T_2.11;
T_2.10 ;
    %mov 8, 2, 1;
T_2.11 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x2041a00_0, 8, 1;
    %jmp T_2.5;
T_2.3 ;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.12, 4;
    %load/x1p 8, v0x2041960_0, 1;
    %jmp T_2.13;
T_2.12 ;
    %mov 8, 2, 1;
T_2.13 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x2041a00_0, 8, 1;
    %jmp T_2.5;
T_2.5 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x2018ea0;
T_3 ;
    %movi 8, 10, 5;
    %ix/get/s 4, 8, 5;
    %cvt/ri 4, 4;
    %set/wr v0x2042880_0, 4;
    %end;
    .thread T_3;
    .scope S_0x2018ea0;
T_4 ;
    %load/wr 4, v0x2042880_0;
    %movi 8, 2, 3;
    %ix/get/s 5, 8, 3;
    %cvt/ri 5, 5;
    %div/wr 4, 5;
    %movi 8, 100, 8;
    %ix/get/s 5, 8, 8;
    %cvt/ri 5, 5;
    %mul/wr 4, 5;
    %cvt/ir 0, 4;
    %delayx 0;
    %load/v 8, v0x20425e0_0, 1;
    %inv 8, 1;
    %set/v v0x20425e0_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x2018ea0;
T_5 ;
    %vpi_call 2 27 "$dumpvars";
    %set/v v0x20425e0_0, 0, 1;
    %set/v v0x20427d0_0, 0, 3;
T_5.0 ;
    %load/v 8, v0x20427d0_0, 3;
   %cmpi/u 8, 4, 3;
    %jmp/0xz T_5.1, 5;
    %load/v 8, v0x20427d0_0, 2; Only need 2 of 3 bits
; Save base=8 wid=2 in lookaside.
    %set/v v0x2042460_0, 8, 2;
    %set/v v0x20421b0_0, 1, 1;
    %load/wr 4, v0x2042880_0;
    %set/wr v0x20423b0_0, 4;
    %fork TD_tb.clkcheck, S_0x2042090;
    %join;
    %ix/load 0, 1, 0;
    %load/vp0 8, v0x20427d0_0, 3;
    %set/v v0x20427d0_0, 8, 3;
    %jmp T_5.0;
T_5.1 ;
    %delay 10000, 0;
    %set/v v0x2042ae0_0, 1, 1;
    %vpi_call 2 37 "$display", "SUCCESS";
    %vpi_call 2 38 "$finish";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "tb.v";
    "top.v";
